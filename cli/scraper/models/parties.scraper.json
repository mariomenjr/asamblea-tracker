{
    "url": "https://www.asamblea.gob.sv",
    "queryObjects": [
        {
            "name": "parlamentaryGroups",
            "endpoint": "grupos-parlamentarios",
            "collections": [
                {
                    "name": "partyDetails",
                    "query": "div.region.region-content > div > section.row > div.col-md-3",
                    "props": [
                        {
                            "name": "partyImage",
                            "query": "div > a.img-rounded.img-thumbnail",
                            "attrs": ["style", "href"]
                        },
                        {
                            "name": "partyHash",
                            "query": "div.btn-gp.text-center > a",
                            "attrs": ["href"]
                        }
                    ]
                }
            ]
        }
    ]
}
